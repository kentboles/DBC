<SystemSettings xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.datacontract.org/2004/07/Poseidon.Configuration">
  <RunScreenSettings>
    <EstimatedReagentUsage>Show</EstimatedReagentUsage>
    <LotNumbers>Show</LotNumbers>
    <SystemDiagnostics>Show</SystemDiagnostics>
    <WashTest>Show</WashTest>
    <BottleLevels>Hide</BottleLevels>
    <InjectionHeadTest>Show</InjectionHeadTest>
  </RunScreenSettings>
  <RunSetupDefaultSettings>
    <SequenceFileLayout>ColumnMajor</SequenceFileLayout>
    <OxidizerSettings>
      <OxidizerSettings>
        <OxidizerType>Oxygen</OxidizerType>
        <Symbol>;</Symbol>
        <ReagentID>REAGENT-5</ReagentID>
        <UseAsDefault>true</UseAsDefault>
      </OxidizerSettings>
      <OxidizerSettings>
        <OxidizerType>Sulphur</OxidizerType>
        <Symbol>*</Symbol>
        <ReagentID>REAGENT-7</ReagentID>
        <UseAsDefault>false</UseAsDefault>
      </OxidizerSettings>
    </OxidizerSettings>
    <DefaultTritylMode>TritylOff</DefaultTritylMode>
    <DefaultSupportType>Universal</DefaultSupportType>
    <ReportLog>Enabled</ReportLog>
    <SequenceSensingDirection>Dir_5_TO_3</SequenceSensingDirection>
  </RunSetupDefaultSettings>
  <PowerSettings>
    <MonitorUPS>false</MonitorUPS>
    <ActionDelay>PT1M</ActionDelay>
    <Action>PauseAtEndOfCycle</Action>
  </PowerSettings>
  <InactivityLockout>999</InactivityLockout>
  <EnableWebServices>true</EnableWebServices>
  <Language>en-US</Language>
  <DrainWasteFrequency>120</DrainWasteFrequency>
  <DrainWasteDuration>5</DrainWasteDuration>
  <UseVariableVolumeWash>false</UseVariableVolumeWash>
  <DefaultVariableVolumeWashReagentID>REAGENT-6</DefaultVariableVolumeWashReagentID>
  <OxidizationDisabled>false</OxidizationDisabled>
  <InterValveInjectionDelay>30</InterValveInjectionDelay>
  <ReagentDefaultDensity>0.786</ReagentDefaultDensity>
  <CalibrateInjectionsBy>Time</CalibrateInjectionsBy>
  <CouplingIdleMinMoveDuration i:nil="true" />
  <RemoteConnectionTimeout>5000</RemoteConnectionTimeout>
</SystemSettings>